{
  "name": "ultimate-guitar/yii2-pq",
  "description": "A paged query class for Yii2 to deal with PHP7 changed in mysqlnd",
  "keywords": ["yii2", "pagination", "query", "db"],
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Sammaye",
      "homepage": "https://www.sammaye.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "yiisoft/db": "^1.0",
    "yidas/yii2-composer-bower-skip": "^2.0",
    "yiisoft/yii2": "^2.0"
  }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {"sammaye\\pq\\": ""}
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
